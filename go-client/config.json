        "type": "databases",
        "path": "",
        "arg_format": "-con name={name}|driver={protocol}|user={username}|password={value}|database={dbname}|host={host}|port={port}|save=false|connect=true",
        "is_internal": false,
        "is_default": false,
        "is_set": false
