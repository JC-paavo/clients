        "type": "databases",
        "path": "",
        "arg_format": "userid={username}/{value}@{host}:{port}/{dbname}",
        "is_internal": false,
        "is_default": false,
        "is_set": false
      },
      {
        "name": "dbeaver",
        "display_name": "DBeaver Community",
        "protocol": [
          "oracle",
          "mysql",
          "postgresql",
          "mariadb",
          "oracle"
