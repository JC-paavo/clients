{
  "filename": "Jumpserve Clients Config",
  "windows": {
    "terminal": [
      {
        "name": "xshell",
        "display_name": "XShell",
        "protocol": [
          "ssh",
          "telnet"
        ],
        "comment": "XShell是一个强大的安全终端模拟软件，它支持SSH、TELNET协议。",
        "type": "linux",
        "path": "",
        "arg_format": "-newtab {name} -url {protocol}://{username}:{value}@{host}:{port}",
        "is_internal": false,
        "is_default": false,
        "is_set": false
      },
      {
        "name": "putty",
        "display_name": "PuTTY",
        "protocol": [
          "ssh",
          "telnet"
        ],
        "comment": "PuTTY是一个Telnet、SSH、rlogin纯TCP以及串行阜连线软件。",
        "type": "linux",
        "path": "putty.exe",
        "arg_format": "-{protocol} {username}@{host} -P {port} -pw {value}",
        "is_internal": true,
        "is_default": true,
        "is_set": true
      }
    ],
    "remotedesktop": [
      {
        "name": "mstsc",
